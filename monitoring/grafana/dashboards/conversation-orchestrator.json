{
  "dashboard": {
    "id": null,
    "title": "ADF-W10 Conversation Orchestrator",
    "tags": ["conversation", "orchestrator", "adf", "ai"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Conversation Orchestrator Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(conversation_turns_total)",
            "legendFormat": "Total Turns"
          },
          {
            "expr": "sum(rate(conversation_turns_total[5m]))",
            "legendFormat": "Turns/sec"
          },
          {
            "expr": "sum(conversation_queue_depth)",
            "legendFormat": "Queue Depth"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Conversation Turns by Outcome",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(conversation_turns_total[5m])) by (outcome)",
            "legendFormat": "{{outcome}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Turn Processing Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(conversation_turn_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(conversation_turn_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(conversation_turn_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "title": "Queue Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "conversation_queue_depth{queue_type=\"waiting\"}",
            "legendFormat": "Waiting"
          },
          {
            "expr": "conversation_queue_depth{queue_type=\"active\"}",
            "legendFormat": "Active"
          },
          {
            "expr": "conversation_queue_depth{queue_type=\"failed\"}",
            "legendFormat": "Failed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "AI Service Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ai_service_requests_total[5m])) by (status)",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        }
      },
      {
        "id": 6,
        "title": "Circuit Breaker States",
        "type": "stat",
        "targets": [
          {
            "expr": "circuit_breaker_state",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "CLOSED",
                    "color": "green"
                  },
                  "1": {
                    "text": "HALF-OPEN",
                    "color": "yellow"
                  },
                  "2": {
                    "text": "OPEN",
                    "color": "red"
                  }
                },
                "type": "value"
              }
            ],
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "AI Token Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ai_tokens_used_total[5m])) by (model)",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 8,
        "title": "Conversation Outcomes by Dealership",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(conversation_outcomes_total) by (outcome)",
            "legendFormat": "{{outcome}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "AI Cost by Model",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ai_cost_total_usd[1h])) by (model)",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "min": 0
          }
        }
      },
      {
        "id": 10,
        "title": "Prompt Template Usage",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(prompt_template_usage_total) by (template_name))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Turn Processing Time by Dealership",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(conversation_turn_duration_seconds_bucket[5m])) by (le, dealership_id)",
            "legendFormat": "{{dealership_id}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Error Rate by AI Model",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(ai_service_requests_total{status=\"error\"}[5m])) by (model) / sum(rate(ai_service_requests_total[5m])) by (model)",
            "legendFormat": "{{model}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.01
                },
                {
                  "color": "red",
                  "value": 0.05
                }
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Conversation Flow Analysis",
        "type": "sankey",
        "targets": [
          {
            "expr": "sum(conversation_turns_total) by (turn_number, outcome)",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "dealership",
          "type": "query",
          "query": "label_values(conversation_turns_total, dealership_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "ai_model",
          "type": "query",
          "query": "label_values(conversation_turns_total, ai_model)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(up[1d]) > 0",
          "titleFormat": "Deployment",
          "textFormat": "Service restarted",
          "iconColor": "blue"
        },
        {
          "name": "Circuit Breaker Events",
          "datasource": "Prometheus",
          "expr": "changes(circuit_breaker_state[1h]) > 0",
          "titleFormat": "Circuit Breaker: {{service}}",
          "textFormat": "State changed",
          "iconColor": "red"
        }
      ]
    },
    "links": [
      {
        "title": "AI Service Dashboard",
        "url": "/d/ai-service/ai-service-dashboard",
        "type": "dashboards"
      },
      {
        "title": "ADF Processing Dashboard", 
        "url": "/d/adf-processing/adf-processing-dashboard",
        "type": "dashboards"
      },
      {
        "title": "System Health Overview",
        "url": "/d/system-health/system-health-overview",
        "type": "dashboards"
      }
    ]
  }
}