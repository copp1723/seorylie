{
  "name": "seorylie",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:server && pnpm run build:frontend && node scripts/build-production.js && pnpm run build:verify",
    "build:verify": "node scripts/verify-build.js",
    "build:old": "cd web-console && NODE_ENV=development npm install && npm run build && cd .. && npm run build:server",
    "build:server": "node config/build/esbuild.config.js",
    "build:frontend": "pnpm --filter seorylie-web-console build",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "start:production": "node scripts/pre-start-check.js && node dist/index.js",
    "start:simple": "node dist/index.js",
    "setup:db": "node setup-db.js",
    "setup:ga4": "node setup-ga4-credentials.js",
    "setup:ga4-simple": "node setup-ga4-simple.js",
    "dev": "DATABASE_URL=postgresql://postgres:password@localhost:5432/seorylie node server-with-db.js",
    "migrate": "node scripts/run-migrations.js",
    "migrate:check": "node scripts/migration-cleanup.js",
    "db:migrate": "ts-node scripts/run-migrations.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:coverage:threshold": "vitest run --coverage --coverage.thresholds.lines=80",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "schema:check": "tsx scripts/compare-schema.ts",
    "schema:ci": "tsx scripts/ci-schema-check.ts",
    "schema:validate": "pnpm run schema:check && pnpm run circular:check",
    "circular:check": "madge --circular --extensions ts,tsx shared/ server/ web-console/src/",
    "deps:check": "npx tsc --noEmit --skipLibCheck",
    "deps:verify": "pnpm list --depth=0 && pnpm audit",
    "deps:install-missing": "pnpm add -w drizzle-kit inquirer archiver bcrypt cookie-parser csv-parser mailparser redis ajv-formats && pnpm add -w -D @playwright/test @testing-library/react jsdom @types/archiver @types/bcrypt @types/cookie-parser @types/mailparser @types/inquirer",
    "precommit": "pnpm run test && pnpm run schema:validate",
    "prepare": "husky"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@google-analytics/data": "^5.1.0",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.50.0",
    "ajv-formats": "^3.0.1",
    "archiver": "^7.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^3.0.0",
    "docxtemplater": "^3.65.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.35.0",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "googleapis": "^150.0.1",
    "helmet": "^8.1.0",
    "inquirer": "^12.6.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nanoid": "^5.1.5",
    "node-cron": "^4.1.0",
    "nodemailer": "^6.9.8",
    "openai": "^4.67.3",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "pizzip": "^3.2.0",
    "prom-client": "^15.1.3",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.1",
    "@testing-library/react": "^16.3.0",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mailparser": "^3.4.6",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autocannon": "^8.0.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.29.0",
    "express-session": "^1.18.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.6.1",
    "madge": "^8.0.0",
    "postgres": "^3.4.7",
    "ts-node": "^10.9.2",
    "vitest": "^3.2.4"
  }
}
